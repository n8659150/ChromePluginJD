{
	"manifest_version": 2,
	"name": "chrome京东搜索插件",
	"version": "1.0",
	"description": "按关键字返回在京东的搜索结果并默认按销量排序",
	"icons": {
		"16": "images/icon16.png",
		"48": "images/icon48.png",
		"128": "images/icon128.png"
	},
	"browser_action": {
	"default_icon": {
		"19": "images/icon19.png",
		"38": "images/icon38.png"
	},
	"permissions": [
    "tabs", "http://*/*", "https://*/*"
  	],
	"default_title": "JD搜索",
	"default_popup": "input.html"
	}
}